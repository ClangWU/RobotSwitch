<launch>
  <!-- Left MCU Node -->
  <node name="left_mcu_node" pkg="robotswitch_controller" type="left_mcu_node" output="screen">
    <param name="left_port_"  value="/dev/ttyUSB1"/>
    <param name="left_baud_"  value="115200"/>
  </node>

  <!-- Right MCU Node -->
  <node name="right_mcu_node" pkg="robotswitch_controller" type="right_mcu_node" output="screen">
    <param name="right_port_"  value="/dev/ttyUSB2"/>
    <param name="right_baud_"  value="115200"/>
  </node>

  <!-- Force MCU Node -->
  <node name="force_mcu_node" pkg="robotswitch_controller" type="force_mcu_node" output="screen">
    <param name="force_port_"  value="/dev/ttyUSB3"/>
    <param name="force_baud_"  value="115200"/>
  </node>

  <!-- Center Node -->
  <node name="center_node" pkg="robotswitch_controller" type="center_node" output="screen">
  </node>
</launch>
